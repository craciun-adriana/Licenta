@page

@section Styles {
    <link rel="stylesheet" href="~/css/Login.css" />
}

@model LicentaUI.Pages.LoginModel
@{
}

<h1>Welcome to ...!</h1>

<form method="post">
    <div class="content">
        <span>UserName</span>
        <input type="text" asp-for="LoginUserModel.UserName">
    </div>

    <div class="content" id="pass">
        <span>Password</span>
        <input type="text" asp-for="LoginUserModel.Password">
    </div>
    <div class="content">
        <span>Remember me</span>
        <input type="checkbox" asp-for="LoginUserModel.RememberMe" />
    </div>
    <div id="logincontent">
        <button id="login" type="submit">Login</button>
    </div>
</form>

<div class="content" id="register">
    <a> Don't have an account yet? Register</a>
</div>